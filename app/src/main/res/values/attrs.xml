<resources>
    <declare-styleable name="LineLayoutL">
        <attr name="pointDrawableNoPassL" format="color|reference" />
        <attr name="pointDrawablePassedL" format="color|reference" />
        <attr name="pointDrawablePassingL" format="color|reference" />
        <attr name="lineDrawableNoPassL" format="color|reference" />
        <attr name="lineDrawablePassedL" format="color|reference" />
        <attr name="lineDrawablePassingL" format="color|reference" />

        <attr name="startDrawable" format="reference" />
        <attr name="endDrawable" format="reference" />
        <attr name="startEndMod" format="enum">
            <enum name="top" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="startEndWidth" format="dimension" />
        <attr name="startEndHeight" format="dimension" />
        <attr name="pointAnim" format="reference" />
        <attr name="pointAnimWidth" format="dimension" />
        <attr name="pointAnimHeight" format="dimension" />
        <attr name="enterOutDrawable" format="reference" />
        <attr name="enterOutWidth" format="dimension" />
        <attr name="enterOutHeight" format="dimension" />
        <attr name="enterOutRate" format="integer" />
    </declare-styleable>

    <declare-styleable name="LineLayoutU">
        <attr name="pointDrawableNoPassU" format="color|reference" />
        <attr name="pointDrawablePassedU" format="color|reference" />
        <attr name="pointDrawablePassingU" format="color|reference" />
        <attr name="lineDrawableNoPassU" format="color|reference" />
        <attr name="lineDrawablePassedU" format="color|reference" />
        <attr name="lineDrawablePassingU" format="color|reference" />

        <attr name="topDrawableNoPassU" format="color|reference" />
        <attr name="topDrawablePassedU" format="color|reference" />
        <attr name="topDrawablePassingU" format="color|reference" />
        <attr name="bottomDrawableNoPassU" format="color|reference" />
        <attr name="bottomDrawablePassedU" format="color|reference" />
        <attr name="bottomDrawablePassingU" format="color|reference" />

        <attr name="lineViewHeight" format="dimension" />
        <attr name="tipsNameLayoutHeight" format="dimension" />

        <attr name="startDrawableU" format="reference" />
        <attr name="endDrawableU" format="reference" />
        <attr name="startEndModU" format="enum">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
        </attr>
        <attr name="startEndWidthU" format="dimension" />
        <attr name="startEndHeightU" format="dimension" />

        <attr name="enterOutDrawableU" format="reference" />
        <attr name="enterOutWidthU" format="dimension" />
        <attr name="enterOutHeightU" format="dimension" />
        <attr name="enterOutRateU" format="integer" />
    </declare-styleable>

    <declare-styleable name="LineLayoutR">
        <attr name="pointDrawableNoPassR" format="color|reference" />
        <attr name="pointDrawablePassedR" format="color|reference" />
        <attr name="pointDrawablePassingR" format="color|reference" />
        <attr name="lineDrawableNoPassR" format="color|reference" />
        <attr name="lineDrawablePassedR" format="color|reference" />
        <attr name="lineDrawablePassingR" format="color|reference" />

        <attr name="rightTopDrawableNoPassR" format="color|reference" />
        <attr name="rightTopDrawablePassedR" format="color|reference" />
        <attr name="rightTopDrawablePassingR" format="color|reference" />
        <attr name="rightBottomDrawableNoPassR" format="color|reference" />
        <attr name="rightBottomDrawablePassedR" format="color|reference" />
        <attr name="rightBottomDrawablePassingR" format="color|reference" />
        <attr name="leftTopDrawableNoPassR" format="color|reference" />
        <attr name="leftTopDrawablePassedR" format="color|reference" />
        <attr name="leftTopDrawablePassingR" format="color|reference" />
        <attr name="leftBottomDrawableNoPassR" format="color|reference" />
        <attr name="leftBottomDrawablePassedR" format="color|reference" />
        <attr name="leftBottomDrawablePassingR" format="color|reference" />

        <attr name="pointAnimR" format="reference" />
        <attr name="pointAnimWidthR" format="dimension" />
        <attr name="pointAnimHeightR" format="dimension" />

        <attr name="lineViewHeightR" format="dimension" />
        <attr name="tipsNameLayoutHeightR" format="dimension" />
    </declare-styleable>


    <declare-styleable name="StationNameView">
        <attr name="stationNameString" format="string" />
        <attr name="stationNameSize" format="dimension" />
        <attr name="stationNameColor" format="color" />
        <attr name="stationNameSpeed" format="float" />
        <attr name="stationNameBold" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TipsNameView">
        <attr name="tipsNameString" format="string" />
        <attr name="tipsNameSize" format="dimension" />
        <attr name="tipsNameColor" format="color" />
        <attr name="tipsNameOffset" format="integer" />
        <attr name="tipsNameSpeed" format="float" />
        <attr name="tipsNameBold" format="boolean" />
    </declare-styleable>
</resources>
